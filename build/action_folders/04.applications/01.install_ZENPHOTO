#!/bin/bash

tar xzf "${ZENPHOTO['file']}"
cd "${ZENPHOTO['dir']}"
mkdir -p "${ZEN_DIR}"
mv -f * "${ZEN_DIR}/"

[[ -f "${ZEN_DIR}/LICENSE" ]]           && rm "${ZEN_DIR}/LICENSE"
[[ -f "${ZEN_DIR}/README.md" ]]         && rm "${ZEN_DIR}/README.md"
[[ -f "${ZEN_DIR}/contributing.md" ]]   && rm "${ZEN_DIR}/contributing.md" 
